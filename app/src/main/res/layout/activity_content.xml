<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:openDrawer="end">

    <!--Content-->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorDarkGray">

        <RelativeLayout
            android:id="@+id/action_bar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_50sdp"
            android:layout_alignParentTop="true"
            android:orientation="horizontal"
            android:background="@color/colorDarkGray">

            <androidx.appcompat.widget.AppCompatImageButton
                android:id="@+id/drawer_btn_id"
                android:layout_width="@dimen/_50sdp"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/ic_drawer_menu"
                tools:ignore="ContentDescription" />

            <com.makeover.makeupartist.Home.AutoWritingText
                android:id="@+id/appName"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="@string/app_name"
                android:textColor="@color/colorDarkGold"
                android:textStyle="bold"
                android:textSize="@dimen/_30ssp"
                android:fontFamily="serif-monospace"
                android:layout_gravity="center"
                tools:ignore="RelativeOverlap" />

            <androidx.appcompat.widget.AppCompatImageButton
                android:id="@+id/refresh_btn_id"
                android:layout_width="@dimen/_50sdp"
                android:layout_height="match_parent"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/ic_refresh"
                tools:ignore="ContentDescription" />

        </RelativeLayout>

        <com.makeover.makeupartist.StarAnimationView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="false"
            android:focusable="false"
            android:layout_gravity="top|center_horizontal"/>

        <RelativeLayout
            android:layout_below="@+id/action_bar"
            android:id="@+id/fragment_content_id"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

    </RelativeLayout>


    <!--Drawer-->
    <com.google.android.material.navigation.NavigationView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:background="@android:color/transparent"
        android:fitsSystemWindows="false">

        <androidx.core.widget.NestedScrollView
            android:background="@drawable/bg_gradient"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <!--Home (Start)-->
                <androidx.cardview.widget.CardView
                    android:id="@+id/drawer_home_btn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:onClick="Drawer_Items"
                    app:cardBackgroundColor="@android:color/transparent">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:background="@drawable/shape_drawer_item"
                        tools:ignore="UseCompoundDrawables">

                        <androidx.appcompat.widget.AppCompatImageView
                            app:srcCompat="@drawable/ic_drawer_home"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_50sdp"
                            android:padding="@dimen/_10sdp"
                            android:background="@color/colorTransLightGold"
                            android:layout_gravity="center"
                            tools:ignore="ContentDescription" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/_10sdp"
                            android:text="@string/home_Drawer"
                            android:textColor="#fff"
                            android:textSize="@dimen/_20ssp"/>

                    </androidx.appcompat.widget.LinearLayoutCompat>

                </androidx.cardview.widget.CardView>
                <!--Home (End)-->


                <View
                    android:layout_width="@dimen/_50sdp"
                    android:layout_height="@dimen/_1sdp"
                    android:background="#fff"/>


                <!--Lip Art Ideas (Start)-->
                <androidx.cardview.widget.CardView
                    android:id="@+id/drawer_lip_art_ideas_btn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:onClick="Drawer_Items"
                    app:cardBackgroundColor="@android:color/transparent">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:background="@drawable/shape_drawer_item"
                        tools:ignore="UseCompoundDrawables">

                        <androidx.appcompat.widget.AppCompatImageView
                            app:srcCompat="@drawable/ic_drawer_lips"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_50sdp"
                            android:padding="@dimen/_10sdp"
                            android:background="@color/colorTransLightGold"
                            android:layout_gravity="center"
                            tools:ignore="ContentDescription" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/_10sdp"
                            android:text="@string/lip_Art_Idea_Drawer"
                            android:textColor="#fff"
                            android:textSize="@dimen/_20ssp"/>

                    </androidx.appcompat.widget.LinearLayoutCompat>

                </androidx.cardview.widget.CardView>

                <View
                    android:layout_width="@dimen/_50sdp"
                    android:layout_height="@dimen/_1sdp"
                    android:background="#fff"/>

                <androidx.cardview.widget.CardView
                    android:id="@+id/lipArt_action_panel"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="@android:color/transparent">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/lip_art_photos_btn"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:background="@drawable/shape_drawer_item"
                            android:onClick="Click_Photo"
                            tools:ignore="UseCompoundDrawables">

                            <androidx.appcompat.widget.AppCompatImageView
                                app:srcCompat="@drawable/ic_photo"
                                android:background="@color/colorTransparentGold"
                                android:layout_width="@dimen/_50sdp"
                                android:layout_height="@dimen/_50sdp"
                                android:padding="@dimen/_12sdp"
                                android:layout_gravity="center"
                                tools:ignore="ContentDescription" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_10sdp"
                                android:layout_gravity="center_vertical"
                                android:text="@string/photo_Gallery"
                                android:textColor="#fff"
                                android:textSize="@dimen/_18ssp"/>

                        </androidx.appcompat.widget.LinearLayoutCompat>

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/lip_art_videos_btn"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:background="@drawable/shape_drawer_item"
                            android:onClick="Click_Video"
                            tools:ignore="UseCompoundDrawables">

                            <androidx.appcompat.widget.AppCompatImageView
                                app:srcCompat="@drawable/ic_video"
                                android:background="@color/colorTransparentGold"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_12sdp"
                                android:layout_gravity="center"
                                tools:ignore="ContentDescription" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:padding="@dimen/_10sdp"
                                android:text="@string/video_Gallery"
                                android:textColor="#fff"
                                android:textSize="@dimen/_18ssp"/>

                        </androidx.appcompat.widget.LinearLayoutCompat>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:background="#fff"/>


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </androidx.cardview.widget.CardView>
                <!--Lip Art Ideas (End)-->


                <!--Eye Makeup (Start)-->
                <androidx.cardview.widget.CardView
                    android:id="@+id/drawer_eye_makeup_btn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:onClick="Drawer_Items"
                    app:cardBackgroundColor="@android:color/transparent">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:background="@drawable/shape_drawer_item"
                        tools:ignore="UseCompoundDrawables">

                        <androidx.appcompat.widget.AppCompatImageView
                            app:srcCompat="@drawable/ic_drawer_eye"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_50sdp"
                            android:padding="@dimen/_10sdp"
                            android:background="@color/colorTransLightGold"
                            android:layout_gravity="center"
                            tools:ignore="ContentDescription" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/_10sdp"
                            android:text="@string/eye_Makeup_Drawer"
                            android:textColor="#fff"
                            android:textSize="@dimen/_20ssp"/>

                    </androidx.appcompat.widget.LinearLayoutCompat>

                </androidx.cardview.widget.CardView>

                <View
                    android:layout_width="@dimen/_50sdp"
                    android:layout_height="@dimen/_1sdp"
                    android:background="#fff"/>

                <androidx.cardview.widget.CardView
                    android:id="@+id/eyeMakeup_action_panel"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="@android:color/transparent">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/eye_Makeup_photos_btn"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:background="@drawable/shape_drawer_item"
                            android:onClick="Click_Photo"
                            tools:ignore="UseCompoundDrawables">

                            <androidx.appcompat.widget.AppCompatImageView
                                app:srcCompat="@drawable/ic_photo"
                                android:background="@color/colorTransparentGold"
                                android:layout_width="@dimen/_50sdp"
                                android:layout_height="@dimen/_50sdp"
                                android:padding="@dimen/_12sdp"
                                android:layout_gravity="center"
                                tools:ignore="ContentDescription" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_10sdp"
                                android:layout_gravity="center_vertical"
                                android:text="@string/photo_Gallery"
                                android:textColor="#fff"
                                android:textSize="@dimen/_18sdp"/>

                        </androidx.appcompat.widget.LinearLayoutCompat>

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/eye_Makeup_videos_btn"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:background="@drawable/shape_drawer_item"
                            android:onClick="Click_Video"
                            tools:ignore="UseCompoundDrawables">

                            <androidx.appcompat.widget.AppCompatImageView
                                app:srcCompat="@drawable/ic_video"
                                android:background="@color/colorTransparentGold"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_12sdp"
                                android:layout_gravity="center"
                                tools:ignore="ContentDescription" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:padding="@dimen/_10sdp"
                                android:text="@string/video_Gallery"
                                android:textColor="#fff"
                                android:textSize="@dimen/_18ssp"/>

                        </androidx.appcompat.widget.LinearLayoutCompat>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:background="#fff"/>


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </androidx.cardview.widget.CardView>
                <!--Eye Makeup (End)-->


                <!--Nail Art Designs (Start)-->
                <androidx.cardview.widget.CardView
                    android:id="@+id/drawer_nail_art_designs_btn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:onClick="Drawer_Items"
                    app:cardBackgroundColor="@android:color/transparent">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:background="@drawable/shape_drawer_item"
                        tools:ignore="UseCompoundDrawables">

                        <androidx.appcompat.widget.AppCompatImageView
                            app:srcCompat="@drawable/ic_drawer_nail"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_50sdp"
                            android:padding="@dimen/_10sdp"
                            android:background="@color/colorTransLightGold"
                            android:layout_gravity="center"
                            tools:ignore="ContentDescription" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/_10sdp"
                            android:text="@string/nail_Art_Designs"
                            android:textColor="#fff"
                            android:textSize="@dimen/_20ssp"/>

                    </androidx.appcompat.widget.LinearLayoutCompat>

                </androidx.cardview.widget.CardView>

                <View
                    android:layout_width="@dimen/_50sdp"
                    android:layout_height="@dimen/_1sdp"
                    android:background="#fff"/>

                <androidx.cardview.widget.CardView
                    android:id="@+id/nailArt_action_panel"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="@android:color/transparent">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/nail_art_photos_btn"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:background="@drawable/shape_drawer_item"
                            android:onClick="Click_Photo"
                            tools:ignore="UseCompoundDrawables">

                            <androidx.appcompat.widget.AppCompatImageView
                                app:srcCompat="@drawable/ic_photo"
                                android:background="@color/colorTransparentGold"
                                android:layout_width="@dimen/_50sdp"
                                android:layout_height="@dimen/_50sdp"
                                android:padding="@dimen/_12sdp"
                                android:layout_gravity="center"
                                tools:ignore="ContentDescription" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_10sdp"
                                android:layout_gravity="center_vertical"
                                android:text="@string/photo_Gallery"
                                android:textColor="#fff"
                                android:textSize="@dimen/_18ssp"/>

                        </androidx.appcompat.widget.LinearLayoutCompat>

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/nail_art_videos_btn"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:background="@drawable/shape_drawer_item"
                            android:onClick="Click_Video"
                            tools:ignore="UseCompoundDrawables">

                            <androidx.appcompat.widget.AppCompatImageView
                                app:srcCompat="@drawable/ic_video"
                                android:background="@color/colorTransparentGold"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_12sdp"
                                android:layout_gravity="center"
                                tools:ignore="ContentDescription" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:padding="@dimen/_10sdp"
                                android:text="@string/video_Gallery"
                                android:textColor="#fff"
                                android:textSize="@dimen/_18ssp"/>

                        </androidx.appcompat.widget.LinearLayoutCompat>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:background="#fff"/>


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </androidx.cardview.widget.CardView>
                <!--Nail Art Designs (End)-->


                <!--Mehedi Designs (Start)-->
                <androidx.cardview.widget.CardView
                    android:id="@+id/drawer_mehedi_designs_btn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:onClick="Drawer_Items"
                    app:cardBackgroundColor="@android:color/transparent">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:background="@drawable/shape_drawer_item"
                        tools:ignore="UseCompoundDrawables">

                        <androidx.appcompat.widget.AppCompatImageView
                            app:srcCompat="@drawable/ic_drawer_mehedi"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_50sdp"
                            android:padding="@dimen/_10sdp"
                            android:background="@color/colorTransLightGold"
                            android:layout_gravity="center"
                            tools:ignore="ContentDescription" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/_10sdp"
                            android:text="@string/Mehedi_Designs"
                            android:textColor="#fff"
                            android:textSize="@dimen/_20ssp"/>

                    </androidx.appcompat.widget.LinearLayoutCompat>

                </androidx.cardview.widget.CardView>

                <View
                    android:layout_width="@dimen/_50sdp"
                    android:layout_height="@dimen/_1sdp"
                    android:background="#fff"/>

                <androidx.cardview.widget.CardView
                    android:id="@+id/mehediDesign_action_panel"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="@android:color/transparent">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/mehedi_designs_photos_btn"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:background="@drawable/shape_drawer_item"
                            android:onClick="Click_Photo"
                            tools:ignore="UseCompoundDrawables">

                            <androidx.appcompat.widget.AppCompatImageView
                                app:srcCompat="@drawable/ic_photo"
                                android:background="@color/colorTransparentGold"
                                android:layout_width="@dimen/_50sdp"
                                android:layout_height="@dimen/_50sdp"
                                android:padding="@dimen/_12sdp"
                                android:layout_gravity="center"
                                tools:ignore="ContentDescription" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_10sdp"
                                android:layout_gravity="center_vertical"
                                android:text="@string/photo_Gallery"
                                android:textColor="#fff"
                                android:textSize="@dimen/_18sdp"/>

                        </androidx.appcompat.widget.LinearLayoutCompat>

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/mehedi_designs_videos_btn"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:background="@drawable/shape_drawer_item"
                            android:onClick="Click_Video"
                            tools:ignore="UseCompoundDrawables">

                            <androidx.appcompat.widget.AppCompatImageView
                                app:srcCompat="@drawable/ic_video"
                                android:background="@color/colorTransparentGold"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_12sdp"
                                android:layout_gravity="center"
                                tools:ignore="ContentDescription" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:padding="@dimen/_10sdp"
                                android:text="@string/video_Gallery"
                                android:textColor="#fff"
                                android:textSize="@dimen/_18ssp"/>

                        </androidx.appcompat.widget.LinearLayoutCompat>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:background="#fff"/>


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </androidx.cardview.widget.CardView>
                <!--Mehedi Designs (End)-->


                <!--Hair Style Designs (Start)-->
                <androidx.cardview.widget.CardView
                    android:id="@+id/drawer_hair_style_btn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:onClick="Drawer_Items"
                    app:cardBackgroundColor="@android:color/transparent">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:background="@drawable/shape_drawer_item"
                        tools:ignore="UseCompoundDrawables">

                        <androidx.appcompat.widget.AppCompatImageView
                            app:srcCompat="@drawable/ic_drawer_hair"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_50sdp"
                            android:padding="@dimen/_10sdp"
                            android:background="@color/colorTransLightGold"
                            android:layout_gravity="center"
                            tools:ignore="ContentDescription" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/_10sdp"
                            android:text="@string/hair_Style"
                            android:textColor="#fff"
                            android:textSize="@dimen/_20ssp"/>

                    </androidx.appcompat.widget.LinearLayoutCompat>

                </androidx.cardview.widget.CardView>

                <View
                    android:layout_width="@dimen/_50sdp"
                    android:layout_height="@dimen/_1sdp"
                    android:background="#fff"/>

                <androidx.cardview.widget.CardView
                    android:id="@+id/hairStyle_action_panel"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="@android:color/transparent"
                    tools:ignore="TooManyViews">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/hair_style_photos_btn"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:background="@drawable/shape_drawer_item"
                            android:onClick="Click_Photo"
                            tools:ignore="UseCompoundDrawables">

                            <androidx.appcompat.widget.AppCompatImageView
                                app:srcCompat="@drawable/ic_photo"
                                android:background="@color/colorTransparentGold"
                                android:layout_width="@dimen/_50sdp"
                                android:layout_height="@dimen/_50sdp"
                                android:padding="@dimen/_12sdp"
                                android:layout_gravity="center"
                                tools:ignore="ContentDescription" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_10sdp"
                                android:layout_gravity="center_vertical"
                                android:text="@string/photo_Gallery"
                                android:textColor="#fff"
                                android:textSize="@dimen/_18sdp"/>

                        </androidx.appcompat.widget.LinearLayoutCompat>

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/hair_style_videos_btn"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:background="@drawable/shape_drawer_item"
                            android:onClick="Click_Video"
                            tools:ignore="UseCompoundDrawables">

                            <androidx.appcompat.widget.AppCompatImageView
                                app:srcCompat="@drawable/ic_video"
                                android:background="@color/colorTransparentGold"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_12sdp"
                                android:layout_gravity="center"
                                tools:ignore="ContentDescription" />

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:padding="@dimen/_10sdp"
                                android:text="@string/video_Gallery"
                                android:textColor="#fff"
                                android:textSize="@dimen/_18ssp"/>

                        </androidx.appcompat.widget.LinearLayoutCompat>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1sdp"
                            android:background="#fff"/>


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </androidx.cardview.widget.CardView>
                <!--Hair Style Designs (End)-->


                <!--Share (Start)-->
                <androidx.cardview.widget.CardView
                    android:id="@+id/drawer_share_btn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:onClick="Drawer_Items"
                    app:cardBackgroundColor="@android:color/transparent">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:background="@drawable/shape_drawer_item"
                        tools:ignore="UseCompoundDrawables">

                        <androidx.appcompat.widget.AppCompatImageView
                            app:srcCompat="@drawable/ic_drawer_share"
                            android:background="@color/colorTransLightGold"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_50sdp"
                            android:padding="@dimen/_12sdp"
                            android:layout_gravity="center"
                            tools:ignore="ContentDescription"/>

                        <androidx.appcompat.widget.AppCompatTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:padding="@dimen/_10sdp"
                            android:text="@string/Share_Drawer"
                            android:textColor="#fff"
                            android:textSize="@dimen/_18ssp"/>

                    </androidx.appcompat.widget.LinearLayoutCompat>

                </androidx.cardview.widget.CardView>
                <!--Share (End)-->

            </androidx.appcompat.widget.LinearLayoutCompat>

        </androidx.core.widget.NestedScrollView>

    </com.google.android.material.navigation.NavigationView>

</androidx.drawerlayout.widget.DrawerLayout>
